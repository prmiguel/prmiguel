#!/usr/bin/env bash

file="projects.md"
echo "# Projects" > $file
echo "" >> $file
echo "| Repo | Details | Demo | Tech |" >> $file
echo "| -----|---------|------|------|" >> $file
cat projects.json | jq -r ".[]|\"|[\(.name)](\(.repo))|\(.details)|[:video_camera:](https://prmiguel.github.io/media/\(.image).gif)|\`\(.tech|join(\"\`, \`\"))\`|\"" >> $file
